{
    "name": "laragle/translate",
    "description": "Laravel's localization management package.",
    "keywords": [
        "laravel",
        "translation",
        "localization"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
    {
        "name": "Darwin Luague",
        "email": "darwinluague9001@gmail.com"
    }],
    "require":
    {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.30"
    },
    "autoload":
    {
        "psr-4":
        {
            "Laragle\\Translate\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Laragle\\Translate\\Tests\\": "tests/"
        }
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        },
        "laravel":
        {
            "providers": [
                "Laragle\\Translate\\TranslateServiceProvider"
            ],
            "aliases":
            {
                "Translate": "Laragle\\Translate\\Translate"
            }
        }
    },
    "config":
    {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev":
    {
    }
}
